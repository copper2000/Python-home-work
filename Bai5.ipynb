    "import numpy as np\n",
    "arr = np.array([1, 2, 3, 4, 5, 6, 7])\n",
    "\n",
    "arrOdd1 = []\n",
    "\n",
    "print(arr[4:7])\n",
    "print(arr[2:4])\n",
    "\n",
    "for i in arr:\n",
    "    if (i % 2 != 0):        \n",
    "        arrOdd1.append(i)\n",
    "arrOdd2 = np.append(1, arrOdd1[1:7])\n",
    "\n",
    "print(arrOdd2)\n",
    "\n",
    "# this way is a bit wordy but gives the right answer :) "
   ]
